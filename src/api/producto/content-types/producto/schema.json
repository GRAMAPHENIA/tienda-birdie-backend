{
  "kind": "collectionType",
  "collectionName": "productos",
  "info": {
    "singularName": "producto",
    "pluralName": "productos",
    "displayName": "Producto",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "productName": {
      "type": "string"
    },
    "slug": {
      "type": "uid"
    },
    "description": {
      "type": "text"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "active": {
      "type": "boolean",
      "default": true
    },
    "price": {
      "type": "integer"
    },
    "style": {
      "type": "enumeration",
      "enum": [
        "naturaleza",
        "retratos",
        "abstracto",
        "iconos",
        "colecciones"
      ]
    },
    "format": {
      "type": "enumeration",
      "enum": [
        "png",
        "jpg",
        "svg",
        "pdf"
      ]
    },
    "isFeatured": {
      "type": "boolean"
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::categoria.categoria"
    }
  }
}
